{
  "name": "@fishui/taro-vue",
  "version": "1.0.6",
  "description": "fishui for taro vue",
  "keywords": [
    "taro",
    "vue",
    "vue3"
  ],
  "author": "yijinc <yijinc@qq.com>",
  "homepage": "https://github.com/yijinc/fishui",
  "license": "ISC",
  "files": ["lib"],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.umd.js"
    }
  },
  "typings": "./lib/index.d.ts",
  "style": "./lib/style/style.css",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "bugs": {
    "url": "https://github.com/yijinc/fishui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yijinc/fishui"
  },
  "scripts": {
    "dev": "vite build",
    "build": "vite build --config config/vite.config.ts && vite build --config config/vite.config.css.ts && vite build --config config/vite.config.disperse.ts"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@tarojs/plugin-framework-vue3": "3.5.8",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "vue": "3.2.45"
  },
  "publishConfig": {
    "access": "public"
  }
}
