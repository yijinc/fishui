{
  "name": "@fishui/demo",
  "version": "1.0.0",
  "private": true,
  "description": "fishui react demo",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "start": "taro build --type weapp --watch",
    "build": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "fish",
  "dependencies": {
    "@fishui/taro-react": "*",
    "@fishui/taro-request": "*",
    "@tarojs/plugin-platform-alipay": "3.5.8",
    "@tarojs/plugin-platform-jd": "3.5.8",
    "@tarojs/plugin-platform-qq": "3.5.8",
    "@tarojs/plugin-platform-swan": "3.5.8",
    "@tarojs/plugin-platform-tt": "3.5.8",
    "@tarojs/plugin-platform-weapp": "3.5.8",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@tarojs/cli": "3.5.8",
    "@tarojs/webpack5-runner": "3.5.8",
    "@types/react": "^18.0.26",
    "@types/webpack-env": "^1.13.6",
    "react-refresh": "^0.11.0",
    "webpack": "5.69.0"
  }
}
